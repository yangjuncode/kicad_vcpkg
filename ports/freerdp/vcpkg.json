{
  "name": "freerdp",
  "version-semver": "2.4.0",
  "description": "A free implementation of the Remote Desktop Protocol (RDP)",
  "homepage": "https://github.com/FreeRDP/FreeRDP",
  "supports": "!(arm | uwp)",
  "dependencies": [
    {
      "name": "glib",
      "platform": "!windows"
    },
    "openssl"
  ],
  "features": {
    "urbdrc": {
      "description": "USB redirection channel support",
      "dependencies": [
        "libusb"
      ]
    }
  }
}
